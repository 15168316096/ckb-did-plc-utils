vector Bytes <byte>;
vector Uint8Vec <byte>;
vector BytesVec <Bytes>;

table OffidAuthorization {
  history: BytesVec,
  sig: Bytes,
  signingKeys: Uint8Vec,
}

option BytesOpt (Bytes);

table WitnessArgs {
